    <tags>grid datagrid jquery plugins widgets javascript handlebars bootstrap simple json mvc asp.net</tags>
    <dependencies>
      <dependency id="Handlebars.js" version="1.0.7" />
      <dependency id="jQuery" version="1.8.3" />
    </dependencies>
  </metadata>
</package>