<?xml version="1.0"?>
<package >
  <metadata>
    <id>SimplePagingGrid</id>
    <version>0.5.0.1</version>
    <title>Simple Paging Grid</title>
    <authors>James Randall</authors>
    <owners>James Randall</owners>
